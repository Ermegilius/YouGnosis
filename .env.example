# ======================
# Backend Configuration
# ======================
NODE_ENV=development
PORT=3000
ALLOWED_ORIGINS=http://localhost:8000

# ======================
# Supabase Configuration
# ======================
SUPABASE_PROJECT_ID=your_project_id_here
SUPABASE_DB_PASSWORD=your_secure_password_here
SUPABASE_URL=https://${SUPABASE_PROJECT_ID}.supabase.co
SUPABASE_STORAGE_URL=https://${SUPABASE_PROJECT_ID}.supabase.co/storage/v1/s3
SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_SECRET_KEY_DEFAULT=your_supabase_secret_key_here

# ======================
# Frontend Configuration
# ======================
VITE_NODE_ENV=development
VITE_PORT=8000
VITE_SUPABASE_URL=${SUPABASE_URL}
VITE_SUPABASE_PUBLISHABLE_KEY=${SUPABASE_PUBLISHABLE_KEY}
VITE_API_URL=http://localhost:${PORT}/api
VITE_FRONTEND_URL=http://localhost:8000

# ======================
# Google OAuth Configuration
# ======================
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret_here